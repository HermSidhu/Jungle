<h1>Signup!</h1>

<%= form_for :user, url: '/users' do |f| %>

<ul>
  <li>Name: <%= f.text_field :name %></li>
  <li>Email: <%= f.text_field :email %></li>
  <li>Password: <%= f.password_field :password %></li>
  <li>Password Confirmation: <%= f.password_field :password_confirmation %></li>
  <%= f.submit "Submit" %>
</ul>

<% end %>